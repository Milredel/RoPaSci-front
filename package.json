    "karma-jasmine-html-reporter": "^1.5.0",
    "mocha": "^8.3.0",
    "mock-local-storage": "^1.1.17",
    "nyc": "15.1.0",
    "protractor": "~7.0.0",
    "schematics-scss-migrate": "^1.3.13",
