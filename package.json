  "version": "1.0.0",
  "scripts": {
    "ng": "ng serve",
    "start": "ng serve --host 0.0.0.0 --disableHostCheck --configuration=staging",
    "start:local": "ng serve --host 0.0.0.0 --disableHostCheck --configuration=local",
    "start:staging": "ng serve --host 0.0.0.0 --disableHostCheck --configuration=staging",
    "start:prod": "ng serve --host 0.0.0.0 --disableHostCheck --configuration=production",
    "build:prod": "ng build --prod --aot --configuration=production",
    "build:staging": "ng build --prod --aot --configuration=staging",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"target\": \"es2017\", \"module\": \"commonjs\"}' node node_modules/mocha/bin/_mocha --require ts-node/register 'src/**/*.spec.ts'",
    "test:coverage": "env TS_NODE_COMPILER_OPTIONS='{\"target\": \"es2017\", \"module\": \"commonjs\"}' node node_modules/nyc/bin/nyc --reporter=text node_modules/mocha/bin/_mocha --require ts-node/register 'src/**/*.spec.ts'",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.0",
    "@angular/cdk": "^11.2.3",
    "@angular/common": "~11.0.0",
