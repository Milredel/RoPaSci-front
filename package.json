    "angular-ng-autocomplete": "^2.0.5",
    "esm": "^3.2.25",
    "jwt-decode": "^3.1.2",
    "mdbootstrap": "^4.19.2",
    "moment": "^2.29.1",
    "ngx-bootstrap": "^6.2.0",
    "ngx-valdemort": "^5.0.0",
    "rxjs": "^6.6.6",
    "sweetalert2": "^10.15.5",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.10.2"
  },
